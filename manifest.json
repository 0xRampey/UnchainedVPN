{
	"manifest_version": 2,
	"name": "Unchained VPN",
	"description": "Decentralising internet privacy",
	"version": "0.1",
	"background": {
		"scripts": ["background.js"]
	},
	"permissions": [
		"activeTab",
		"storage",
		"proxy",
		"tabs"
	],
	"icons": {
		"128": "icon.png"
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	}
}
