{
	"manifest_version": 3,
	"name": "Unchained dVPN",
	"description": "Decentralising internet privacy",
	"version": "0.2.0",
	"background": {
		"service_worker": "background.js"
	},
	"permissions": [
		"storage",
		"proxy"
	],
	"icons": {
		"128": "logo.png"
	},
	"action": {
		"default_icon": "logo.png",
		"default_popup": "popup.html"
	}
}
